{
	"name": "server",
	"version": "1.0.0",
	"description": "Bob's Garage API server",
	"main": "dist/server.js",
	"type": "module",
	"license": "ISC",
	"author": "",
	"repository": {
		"type": "git",
		"url": ""
	},
	"keywords": [
		"api",
		"express",
		"typescript",
		"mysql",
		"sequelize",
		"rest",
		"jwt"
	],
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"dev": "nodemon --watch src --ext ts,js --exec tsx src/server.ts",
		"build": "tsc -p tsconfig.json",
		"prebuild": "yarn clean",
		"start": "node dist/server.js",
		"clean": "node -e \"const fs=require('fs');if(fs.existsSync('dist'))fs.rmSync('dist',{recursive:true,force:true});fs.readdirSync('.').filter(f=>f.endsWith('.tsbuildinfo')).forEach(f=>fs.rmSync(f,{force:true}));\" || true",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"seed": "tsx src/db/seeders/run-seeder.ts",
		"migrate": "sequelize-cli db:migrate",
		"migrate:undo": "sequelize-cli db:migrate:undo",
		"migrate:create": "sequelize-cli migration:generate --name",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"lint": "biome lint src",
		"lint:fix": "biome check --write src",
		"format": "biome format --write src"
	},
	"dependencies": {
		"bcrypt": "^6.0.0",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"dotenv": "^17.2.1",
		"express": "^5.1.0",
		"express-rate-limit": "^8.2.1",
		"helmet": "^8.1.0",
		"ioredis": "^5.4.1",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.1",
		"multer": "^2.0.2",
		"mysql2": "^3.14.3",
		"prom-client": "^15.1.0",
		"reflect-metadata": "^0.2.2",
		"sequelize": "^6.37.7",
		"sequelize-typescript": "^2.1.6",
		"winston": "^3.18.3",
		"zod": "^4.0.17"
	},
	"devDependencies": {
		"@types/bcrypt": "^6.0.0",
		"@types/compression": "^1.7.5",
		"@types/cookie-parser": "^1.4.9",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.3",
		"@types/jsdom": "^27.0.0",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/morgan": "^1.9.10",
		"@types/multer": "^2.0.0",
		"@types/node": "^24.3.0",
		"@types/supertest": "^6.0.3",
		"@types/winston": "^2.4.4",
		"nodemon": "^3.1.0",
		"supertest": "^7.1.4",
		"tsx": "^4.20.4",
		"typescript": "^5.9.2",
		"vitest": "^3.2.4"
	}
}
