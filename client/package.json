		"bootstrap": "^5.3.7",
		"framer-motion": "^12.23.24",
		"react": "^19.1.1",
		"react-bootstrap": "^2.10.10",
		"react-dom": "^19.1.1",
		"react-icons": "^5.3.0",
		"react-redux": "^9.2.0",
		"react-router-dom": "^7.8.1",
		"zod": "^4.0.17"
	},
