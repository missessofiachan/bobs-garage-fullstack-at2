  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "prebuild": "yarn clean",
    "preview": "vite preview",
    "typecheck": "tsc -b --noEmit",
    "clean": "node -e \"const fs=require('fs');if(fs.existsSync('dist'))fs.rmSync('dist',{recursive:true,force:true});fs.readdirSync('.').filter(f=>f.endsWith('.tsbuildinfo')).forEach(f=>fs.rmSync(f,{force:true}));\" || true"
  },
